  "keywords": [
    "blake3",
    "hash",
    "wasm",
    "auto",
    "fast"
  ],
  "author": "Rotko Networks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rotkonetworks/blake3-js"
  },
  "homepage": "https://blake3.rotko.net",
  "dependencies": {
    "@rotko/blake3": "^1.0.0",
    "@rotko/blake3-wasm": "^1.0.0"
  }
}
