{
  "name": "@rotko/blake3-wasm",
  "version": "1.0.0",
  "description": "High-performance Blake3 hash - Rust WASM with Rayon multi-threading",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:wasm && npm run build:js",
    "build:wasm": "cd ../../rust-wasm && wasm-pack build --target web --out-dir ../packages/blake3-wasm/dist/wasm",
    "build:js": "cp src/index.js dist/ && cp src/index.d.ts dist/"
  },
  "keywords": [
    "blake3",
    "hash",
    "wasm",
    "rayon",
    "parallel",
    "fast"
  ],
  "author": "Rotko Networks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rotkonetworks/blake3-js"
  },
  "homepage": "https://blake3.rotko.net"
}
